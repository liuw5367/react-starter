{
  "name": "app",
  "version": "1.0.0",
  "description": "quick start for React",
  "private": "true",
  "scripts": {
    "start": "vite",
    "serve": "vite",
    "preview": "vite preview",
    "build": "tsc && vite build",
    "prepare": "husky install",
    "eslint": "eslint src --quiet --ext .js,.jsx,.ts,.tsx",
    "stylelint": "stylelint \"src/**/*.{css,scss,sass,less}\" --quiet --fix",
    "prettier": "prettier --write **/*.{js,jsx,tsx,ts,less,md,json}",
    "test": "jest",
    "type-check": "tsc"
  },
  "lint-staged": {
    "*.{jsx,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ],
    "*.{css,less}": [
      "stylelint --quiet --fix"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --quiet --fix"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
