{
  "name": "app",
  "version": "1.0.0",
  "private": "true",
  "description": "quick start for React",
  "scripts": {
    "start": "vite",
    "serve": "vite",
    "preview": "vite preview",
    "build": "tsc && vite build",
    "prepare": "husky install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix --quiet",
    "lint:style": "stylelint \"src/**/*.{css,scss,sass,less}\" --quiet --fix",
    "prettier": "prettier --write **/*.{js,jsx,tsx,ts,less,md,json}",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "type-check": "tsc"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "ahooks": "^3.7.4",
    "antd": "^5.2.1",
    "axios": "^1.3.3",
    "classnames": "^2.3.2",
    "core-js": "^3.28.0",
    "dayjs": "^1.11.7",
    "immer": "^9.0.19",
    "jotai": "^2.0.2",
    "jotai-immer": "^0.2.0",
    "lodash": "^4.17.21",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.8.1"
  },
  "devDependencies": {
    "@antfu/eslint-config-react": "^0.35.2",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@iconify-json/ant-design": "^1.1.5",
    "@iconify-json/ph": "^1.1.4",
    "@types/lodash": "^4.14.191",
    "@types/mockjs": "^1.0.7",
    "@types/node": "^16.18.12",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-helmet": "^6.1.6",
    "@unocss/preset-attributify": "^0.49.7",
    "@unocss/preset-icons": "^0.49.7",
    "@unocss/transformer-directives": "^0.49.7",
    "@unocss/transformer-variant-group": "^0.49.7",
    "@vitejs/plugin-legacy": "^4.0.1",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.34.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^13.1.2",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.21",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.4",
    "stylelint": "^15.1.0",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-less": "^1.0.6",
    "terser": "^5.16.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "unocss": "^0.49.7",
    "vite": "^4.1.2",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-svgr": "^2.4.0",
    "vitest": "^0.28.5"
  },
  "lint-staged": {
    "*.{jsx,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ],
    "*.{css,less}": [
      "stylelint --quiet --fix"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --quiet --fix"
    ]
  }
}
