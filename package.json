    "core-js": "^3.27.2",
    "dva-core": "^2.0.4",
    "dva-loading": "^3.0.22",
    "history": "^5.3.0",
    "immer": "^9.0.19",
    "jotai": "^2.0.0",
    "jotai-immer": "^0.2.0",
    "lodash": "^4.17.21",
    "mac-scrollbar": "^0.10.4",
    "moment": "^2.29.4",
    "query-string": "^8.1.0",
